<% if (title){ %>
    <h1><%= title %></h1>
  <%- messages() %>
  <% if (errors) { %>
    <ul class="notice">
  <% errors.array().forEach(error => { %>
     <li><%= error.msg %></li>
  <%  }) %>
   </ul>
  <% } %>
  <% } %>

  <h2>Employee List</h2>
    <table>
        <tr>
        <th>Employee ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Account Type</th>
        <th>Account Status</th>
        <th>Manage</th>
        </tr>
        <% locals.employeeList.forEach(employee => { %>
        <tr>
            <td><%= employee.account_id %></td>
            <td><%= employee.account_firstname %></td>
            <td><%= employee.account_lastname %></td>
            <td><%= employee.account_type %></td>
            <td><%= employee.account_status %></td>
            <td><a href="/account/employee-management"<%= employee.account_id %>">Manage</a></td>
        </tr>
        <% }) %>